{
  "name": "joskolenberg/eloquent-empty-date-nullifier",
  "description": "Convert 0000-00-00 dates in the database to null when accessing your model's attributes.",
  "license": "MIT",
  "keywords": ["dates", "laravel", "date", "eloquent"],
  "authors": [
    {
      "name": "Jos Kolenberg",
      "email": "jos@kolenberg.net"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "illuminate/database": "^5.5|^6.0"
  },
  "autoload": {
    "psr-4": {
      "JosKolenberg\\EloquentEmptyDateNullifier\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JosKolenberg\\EloquentEmptyDateNullifier\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0|^7.0|^8.0"
  }
}
